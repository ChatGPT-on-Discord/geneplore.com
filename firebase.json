{
  "hosting": {
    "public": "site",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites":
    [
      {
        "source": "/link",
        "destination": "/link.html"
      },
      {
        "source": "/gift",
        "destination": "/gift.html"
      },
      {
        "source": "/success",
        "destination": "/success.html"
      }
    ],
    "redirects":
    [
      {
        "source": "/discord",
        "destination": "https://discord.gg/RsztF7QRp7",
        "type": 301
      },
      {
        "source": "/bot",
        "destination": "https://discord.com/application-directory/1089317211652509811",
        "type": 301
      },
      {
        "source": "/terms",
        "destination": "https://geneplore.com/terms",
        "type": 301
      },
      {
        "source": "/",
        "destination": "https://geneplore.com",
        "type": 301
      }
      
    ]
  }

}
