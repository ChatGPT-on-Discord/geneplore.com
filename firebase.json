{
  "hosting": {
    "public": "site",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites":
    [
      {
        "source": "/terms",
        "destination": "/terms.html"
      },
      {
        "source": "/link",
        "destination": "/link.html"
      },
      {
        "source": "/gift",
        "destination": "/gift.html"
      }
    ],
    "redirects":
    [
      {
        "source": "/discord",
        "destination": "https://discord.gg/RsztF7QRp7",
        "type": 301
      },
      {
        "source": "/bot",
        "destination": "https://discord.com/application-directory/1089317211652509811",
        "type": 301
      }
      
    ]
  }

}
